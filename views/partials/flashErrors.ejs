<div class="flashMessage">
    <% if (locals.error) { %>
        <div class="alert alert-danger">
          <%= error %>
        </div>
        <% } %>
        
        <% if (locals.success) { %>
          <div class="alert alert-success">
            <%= success %>
          </div>
          <% } %>
          <% if (message) { %>
            <div class="alert alert-success">
              <%= message %>
            </div>
            <% } %>


</div>